         }
         .lay-update {
         background-color: #73a8a0;
         font-weight: bold;
         }
         .no-update {
         background-color:  #ffe200;
         font-weight: bold;
         }
         .yes-update {
         background-color: #002852;
         font-weight: bold;
         }
